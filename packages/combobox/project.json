{
  "name": "combobox",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/combobox/src",
  "projectType": "library",
  "tags": ["type:webcomponent"],
  "generators": {
    "@nxext/stencil:component": {
      "style": "scss"
    }
  },
  "targets": {
    "test": {
      "executor": "@nxext/stencil:test",
      "outputs": ["{options.outputPath}"],
      "options": {
        "projectType": "library",
        "tsConfig": "packages/combobox/tsconfig.lib.json",
        "configPath": "packages/combobox/stencil.config.ts",
        "outputPath": "dist/packages/combobox"
      }
    },
    "build": {
      "executor": "@nxext/stencil:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "projectType": "library",
        "tsConfig": "packages/combobox/tsconfig.lib.json",
        "configPath": "packages/combobox/stencil.config.ts",
        "outputPath": "dist/packages/combobox"
      },
      "configurations": {
        "production": {
          "dev": false,
          "prod": true
        }
      }
    },
    "serve": {
      "executor": "@nxext/stencil:serve",
      "outputs": ["{options.outputPath}"],
      "options": {
        "projectType": "library",
        "tsConfig": "packages/combobox/tsconfig.lib.json",
        "configPath": "packages/combobox/stencil.config.ts",
        "outputPath": "dist/packages/combobox"
      }
    },
    "e2e": {
      "executor": "@nxext/stencil:e2e",
      "outputs": ["{options.outputPath}"],
      "options": {
        "projectType": "library",
        "tsConfig": "packages/combobox/tsconfig.lib.json",
        "configPath": "packages/combobox/stencil.config.ts",
        "outputPath": "dist/packages/combobox"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "packages/combobox/src/**/*.{ts,tsx,json}",
          "packages/combobox/*.{ts,tsx,json}"
        ]
      }
    }
  }
}
